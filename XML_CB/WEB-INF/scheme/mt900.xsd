<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://www.project.com/mt900" xmlns:cmn="http://www.project.com/common_types"
    xmlns:mt900="http://www.project.com/mt900">
    <xs:import namespace="http://www.project.com/common_types" schemaLocation="common_types.xsd"/>
    <xs:element name="mt900">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ID_poruke">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="50"/>
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Podaci_o_zaduzenju">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element default="AAAAAA00" name="SWIFT_kod_banke_duznika"
                                type="cmn:TSwift_kod_banke"/>
                            <xs:element default="000-0000000000000-00"
                                name="Obracunski_racun_banke_duznika"
                                type="cmn:TBroj_Bankarskog_Racuna"/>
                            <xs:element name="ID_poruke_naloga">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                        <xs:minLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="Datum_valute" type="xs:date"/>
                            <xs:element name="Iznos">
                                <xs:simpleType>
                                    <xs:restriction base="xs:decimal">
                                        <xs:totalDigits value="15"/>
                                        <xs:fractionDigits value="2"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="Sifra_valute">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="3"/>
                                        <xs:pattern value="[A-Z]{3}|[0-9]{3}"/>
                                        <xs:enumeration value="EUR"/>
                                        <xs:enumeration value="RSD"/>
                                        <xs:enumeration value="GBP"/>
                                        <xs:enumeration value="CHF"/>
                                        <xs:enumeration value="HUF"/>
                                        <xs:enumeration value="JPY"/>
                                        <xs:enumeration value="TRY"/>
                                        <xs:enumeration value="USD"/>
                                        <xs:enumeration value="AUD"/>
                                        <xs:enumeration value="036"/>
                                        <xs:enumeration value="840"/>
                                        <xs:enumeration value="949"/>
                                        <xs:enumeration value="392"/>
                                        <xs:enumeration value="348"/>
                                        <xs:enumeration value="756"/>
                                        <xs:enumeration value="826"/>
                                        <xs:enumeration value="941"/>
                                        <xs:enumeration value="978"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
