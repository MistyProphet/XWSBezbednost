<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="RTGSDocumentService" 
				  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
				  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
				  xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" 
				  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
				  xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" 
				  xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" 
				  xmlns:inf="http://informatika.ftn.ns.ac.yu/ws/hello" 
				  xmlns:inf_type="http://informatika.ftn.ns.ac.yu/ws/hello/types" 
				  targetNamespace="http://informatika.ftn.ns.ac.yu/ws/hello">
	<wsdl:types>
	<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:inf_type="http://informatika.ftn.ns.ac.yu/ws/hello/types"  elementFormDefault="qualified" version="1.0" >
 	<xsd:import namespace="http://informatika.ftn.ns.ac.yu/ws/hello/types" schemaLocation="mojaSema.xsd" />            
</xsd:schema>
  </wsdl:types>
	<wsdl:message name="RTGSRequest">
		<wsdl:part name="RequestRTGS" element="inf_type:RequestRTGS"/>
	</wsdl:message>
	<wsdl:message name="RTGSResponse">
		<wsdl:part name="ResponseRTGS" element="inf_type:ResponseRTGS"/>
	</wsdl:message>
	<wsdl:portType name="RTGSDocument">
		<wsdl:operation name="requestRTGS">
			<wsdl:input message="inf:RTGSRequest"/>
			<wsdl:output message="inf:RTGSResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="RTGSDocumentServiceSoapBinding" type="inf:RTGSDocument">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="requestRTGS">
			<soap:operation soapAction=""/>
			<wsdl:input>
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="RTGSDocumentService">
		<wsdl:port name="RTGSDocumentPort" binding="inf:RTGSDocumentServiceSoapBinding">
			<soap:address location="http://localhost:8080/Hello"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
